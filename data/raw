# Clone the repository locally
git clone https://github.com/yourusername/ZT.git

# Go to the repository directory
cd ZT

# Navigate to the data/raw folder
cd data/raw

# Copy your dataset file into this folder
cp C:\Users\hp\Downloads\mitbih_test.csv

# Add the file to git
git add .

# Commit your changes
git commit -m "Add dataset file"

# Push to GitHub
git push origin main
